{
  "name": "simplforms",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "next start",
    "lint": "next lint",
    "dev": "concurrently \"next dev \" \"tailwind --watch\"",
    "build": "tailwind && next build",
    "tailwind": "tailwindcss --input tailwind.css --output app/globals.css",
    "start:prisma-studio": "npx prisma studio",
    "seed:db": "npx prisma db seed",
    "db:migrate": "npx prisma migrate dev",
    "db:push": "npx prisma db push"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --ext ts --ext tsx --ext js"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run eslint"
    }
  },
  "dependencies": {
    "@ant-design/icons": "^5.0.1",
    "@apollo/client": "^3.7.8",
    "@next/font": "13.1.6",
    "@prisma/client": "4.10.1",
    "@supabase/auth-helpers-nextjs": "^0.5.4",
    "@supabase/auth-helpers-react": "^0.3.1",
    "@supabase/auth-ui-react": "^0.2.8",
    "@supabase/supabase-js": "^2.8.0",
    "@types/node": "18.13.0",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "antd": "^5.2.2",
    "apollo-server-micro": "^3.11.1",
    "concurrently": "7.6.0",
    "encoding": "^0.1.13",
    "eslint": "8.34.0",
    "eslint-config-next": "^13.1.6",
    "graphql": "^16.6.0",
    "graphql-tools": "^8.3.18",
    "micro": "^9.3.4",
    "micro-cors": "^0.1.1",
    "next": "13.1.6",
    "nexus": "^1.3.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "4.9.5"
  },
  "overrides": {
    "graphql": "^16.6.0"
  },
  "devDependencies": {
    "@svgr/webpack": "^6.5.1",
    "@types/micro-cors": "^0.1.3",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "autoprefixer": "10.4.13",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.2",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "prisma": "^4.10.1",
    "tailwindcss": "3.2.7",
    "ts-node": "^10.9.1"
  }
}
